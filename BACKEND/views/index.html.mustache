<body data-spy="scroll" data-target=".bs-docs-sidebar">

<!-- begin nav header -->
{{{content}}}
<!-- end of nav header -->

<div class="container">
    <div id="indexes" class="row marketing">
        {{#repositories}}
            <div class="span4 card" {{^LogoImage}}style="padding-top:10px;"{{/LogoImage}}>
                {{#LogoImage}}
                    <div class="card-media" >
                        <a class="card-media-container" href="{{SITEURL}}/{{Slug}}.html" target="_blank">
                            <img src="{{LogoImage}}" alt="{{Title}}"/>
                        </a>
                    </div>
                {{/LogoImage}}

                <div class="card-body" {{^LogoImage}}style="margin-top:0px;"{{/LogoImage}}>
                    <h2><a href="{{SITEURL}}/{{Slug}}.html" target="_blank">{{Title}}</a></h2>
                    <span style="display:inline-block;padding-bottom:5px;">
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 8 8" style="vertical-align:middle;">
                        <path d="M4 0l-1 3h-3l2.5 2-1 3 2.5-2 2.5 2-1-3 2.5-2h-3l-1-3z" />
                    </svg>
                    <strong style="vertical-align:middle;">{{StarCount}}</strong>
                </span>
                    <p>{{Summary}}</p>
                </div>
            </div>
        {{/repositories}}
    </div>

    <div id="loading" class="row"></div>

    <nav id="pagination">
        {{#nextpagelink}}
            <a class="next" href="{{nextpagelink}}">Next >></a>
        {{/nextpagelink}}
    </nav>
</div>

