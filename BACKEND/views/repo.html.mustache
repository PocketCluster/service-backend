<div class="row">

    <div class="span9">
        <section>
            <div class="page-header" style="display:inline-block;margin:0 0 20px;">
                <h1>{{repo.Title}}</h1>

                <div>
                    <span style="position:relative;display:inline-block;padding-right:1em;text-align:left;">
                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 8 8" style="vertical-align:middle;">
                            <path d="M1.5 0c-.83 0-1.5.67-1.5 1.5 0 .66.41 1.2 1 1.41v2.19c-.59.2-1 .75-1 1.41 0 .83.67 1.5 1.5 1.5s1.5-.67 1.5-1.5c0-.6-.34-1.1-.84-1.34.09-.09.21-.16.34-.16h2c.82 0 1.5-.68 1.5-1.5v-.59c.59-.2 1-.75 1-1.41 0-.83-.67-1.5-1.5-1.5s-1.5.67-1.5 1.5c0 .66.41 1.2 1 1.41v.59c0 .28-.22.5-.5.5h-2c-.17 0-.35.04-.5.09v-1.19c.59-.2 1-.75 1-1.41 0-.83-.67-1.5-1.5-1.5z"/>
                        </svg>
                        <strong style="vertical-align:middle;">{{repo.ForkCount}}</strong>
                    </span>

                    <span style="position:relative;display:inline-block;padding-right:1em;text-align:left;">
                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 8 8" style="vertical-align:middle;">
                            <path d="M4 0l-1 3h-3l2.5 2-1 3 2.5-2 2.5 2-1-3 2.5-2h-3l-1-3z" />
                        </svg>
                        <strong style="vertical-align:middle;">{{repo.StarCount}}</strong>
                    </span>

                    <span style="position:relative;display:inline-block;text-align:left;">
                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 8 8" style="vertical-align:middle;">
                            <path d="M4.03 0c-2.53 0-4.03 3-4.03 3s1.5 3 4.03 3c2.47 0 3.97-3 3.97-3s-1.5-3-3.97-3zm-.03 1c1.11 0 2 .9 2 2 0 1.11-.89 2-2 2-1.1 0-2-.89-2-2 0-1.1.9-2 2-2zm0 1c-.55 0-1 .45-1 1s.45 1 1 1 1-.45 1-1c0-.1-.04-.19-.06-.28-.08.16-.24.28-.44.28-.28 0-.5-.22-.5-.5 0-.2.12-.36.28-.44-.09-.03-.18-.06-.28-.06z"  transform="translate(0 1)" />
                        </svg>
                        <strong style="vertical-align:middle;">{{repo.WatchCount}}</strong>
                    </span>

                    <!--span style="position:relative;display:inline-block;vertical-align:middle;">
                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 8 8">
                            <path d="M5 0l-3 6h1l3-6h-1zm-4 1l-1 2 1 2h1l-1-2 1-2h-1zm5 0l1 2-1 2h1l1-2-1-2h-1z" transform="translate(0 1)" />
                        </svg>
                        <strong>4</strong>
                    </span>

                    <span style="position:relative;display:inline-block;vertical-align:middle;">
                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 8 8">
                            <path d="M3.09 0c-.06 0-.1.04-.13.09l-2.94 6.81c-.02.05-.03.13-.03.19v.81c0 .05.04.09.09.09h6.81c.05 0 .09-.04.09-.09v-.81c0-.05-.01-.14-.03-.19l-2.94-6.81c-.02-.05-.07-.09-.13-.09h-.81zm-.09 3h1v2h-1v-2zm0 3h1v1h-1v-1z" />
                        </svg>
                        <strong>4</strong>
                    </span-->
                </div>
            </div>

            <div class="row-fluid">

                <div class="span8">

                    {{#repo.logoimage}}
                    <img src="{{repo.logoimage}}" alt="{{repo.title}}" />
                    {{/repo.logoimage}}

                    <span>
                    {{#repo.ProjectPage}}
                        <a class="btn btn-primary" href="{{repo.ProjectPage}}">Project Page</a>
                    {{/repo.ProjectPage}}

                    <a class="btn" href="{{repo.RepoPage}}">GitHub <i class="icon-github"></i></a>

                    </span>
                </div>

                <div class="span4">
                    {{#repo.RepoContributor}}
                    <div class="row-fluid">
                        {{#repo.RepoContributor}}
                        <div class="col-md-1">
                            <a class="pull-left" href="https://github.com/{{author}}">
                                <img class="media-object" src="https://avatars.githubusercontent.com/{{author}}?s=60" alt="{{author}}" style="height:60px;width:60px;margin:0.2em;">
                            </a>
                        </div>
                        {{/repo.RepoContributor}}
                    </div>
                    {{/repo.RepoContributor}}
                </div>

            </div>

            <div class="row-fluid">

                <div class="span12">
                    <p class="lead">{{{readme}}}</p>
                </div>

            </div>

        </section>
    </div>

    <div class="span3">

    </div>
</div>